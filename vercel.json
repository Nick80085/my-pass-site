{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/success", "dest": "/success.html" },
    { "src": "/cancel", "dest": "/cancel.html" },
    { "src": "/", "dest": "/index.html" }
  ]
}
